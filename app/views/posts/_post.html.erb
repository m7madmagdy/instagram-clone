<div id="<%= dom_id post %>" class="post-container">
  <h5 style="text-align: left;">
    <%= post.user.email %>
  </h5>
  <p>
    <%= post.title %>
  </p>
  <p>
    <%= post.description %>
  </p>
  <p>
    <%= post.keywords %>
  </p>
  <p>
    <%= link_to "", post, class: "post-link" %>
  </p>
  <% post.images.each do |image| %>
    <%= image_tag(image, style: "object-fit: cover;") %>
  <% end %>
</div>
