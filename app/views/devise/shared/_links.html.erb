<%- if controller_name != 'sessions' %>
  <br/>
  <span>
    <pre>Alerdy have an account?<strong><%= link_to "Log in", new_session_path(resource_name), class: "nav-link text-primary" %></strong></pre>
  </span>
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <div>
    <pre>Don't have account? <strong><%= link_to "Sign up", new_registration_path(resource_name), class: "nav-link text-primary" %></strong></pre>
  </div>
<% end %>
